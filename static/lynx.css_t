img.screenshot {
}

tt.descname, tt.descclassname {
    font-size: 0.95em;
}

tt.descname {
    padding-right: 0.08em;
}

img.screenshot {
    -moz-box-shadow: 2px 2px 4px #eee;
    -webkit-box-shadow: 2px 2px 4px #eee;
    box-shadow: 2px 2px 4px #eee;
}

table.docutils {
    border: 1px solid #888;
    -moz-box-shadow: 2px 2px 4px #eee;
    -webkit-box-shadow: 2px 2px 4px #eee;
    box-shadow: 2px 2px 4px #eee;
}

table.docutils td, table.docutils th {
    border: 1px solid #888;
    padding: 0.25em 0.7em;
}

table.field-list, table.footnote {
    border: none;
    -moz-box-shadow: none;
    -webkit-box-shadow: none;
    box-shadow: none;
}

table.footnote {
    margin: 15px 0;
    width: 100%;
    border: 1px solid #eee;
    background: #fdfdfd;
    font-size: 0.9em;
}

table.footnote + table.footnote {
    margin-top: -15px;
    border-top: none;
}

table.field-list th {
    padding: 0 0.8em 0 0;
}

table.field-list td {
    padding: 0;
}

table.footnote td.label {
    width: 0px;
    padding: 0.3em 0 0.3em 0.5em;
}

table.footnote td {
    padding: 0.3em 0.5em;
}

dl {
    margin: 0;
    padding: 0;
}

dl dd {
    margin-left: 30px;
}

dl dt {
    font-weight: bold;
}

blockquote {
    margin: 0 0 0 30px;
    padding: 0;
}

ul, ol {
    margin: 10px 0 10px 30px;
    padding: 0;
}

pre {
    padding: 7px 30px;
    line-height: 1.3em;
    background: #fdfdfd;

}


tt.xref, a tt {
    background-color: #FBFBFB;
    border-bottom: 1px solid white;
}

a.reference {
    text-decoration: none;
    border-bottom: 1px dotted #004B6B;
}

a.reference:hover {
    border-bottom: 1px solid #6D4100;
}

a.footnote-reference {
    text-decoration: none;
    font-size: 0.7em;
    vertical-align: top;
    border-bottom: 1px dotted #004B6B;
}

a.footnote-reference:hover {
    border-bottom: 1px solid #6D4100;
}

a:hover tt {
    background: #EEE;
}

/* --- Github / Bitbucket forks --- */

.fork {
    border: none;
    position: fixed;
    right: 0;
    top: 0;
}



div.sphinxsidebarwrapper a.reference,
div.sphinxsidebarwrapper a.reference:hover
 {
    text-decoration: none;
    border: none;;
}

div.sphinxsidebarwrapper a{
    color: #555;
    text-decoration: none;
}
div.sphinxsidebarwrapper a:hover{
    text-decoration: underline;
}
div.sphinxsidebarwrapper ul{
    border-top: 1px dotted #acacac;
}
div.sphinxsidebarwrapper ul li{
    padding: 9px 0;
    border-bottom: 1px dotted #acacac;
    background: url(sidebar-bullet.png) 0 17px no-repeat;

}

div.sphinxsidebarwrapper ul li a{
    display: block;
    margin-left: 10px;
}
div.sphinxsidebarwrapper ul ul{
    padding: 9px 0 0 10px;
    margin: 9px 0 0;
}
div.sphinxsidebarwrapper ul ul li{
    padding: 4px 0 5px 15px;
    margin: 0;
    border: none;
    background: url(sidebar-li-li.png) no-repeat;
    list-style: none;
}
div.sphinxsidebarwrapper ul ul li a{
    margin-left: 0;
}
